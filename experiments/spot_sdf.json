{
    "description": "SIREN Learning of the Armadillo mesh using true SDF values and non-uniform sampling.",
    "num_epochs": 2001,
    "sampling_opts": {
        "samples_on_surface": 2930,
        "samples_off_surface": 2930,
        "random_surf_samples": true,
        "sampler": false
    },
    "dataset": "spot.ply",
    "batch_size": 2930,
    "scaling": "",
    "checkpoint_path": "logs",
    "experiment_name": "spot_1x64_w0-16",
    "epochs_to_checkpoint": 100,
    "epochs_to_reconstruction": [10, 100, 500, 1000, 2000],
    "loss": "sdf",
    "optimizer": {
        "type": "adam",
        "lr": 1e-4
    },
    "reconstruction": {
        "resolution": 256,
        "output_file": "final"
    },
    "network": {
        "hidden_layer_nodes": [64, 64],
        "w0": 16,
        "ww": 16
    }
}
